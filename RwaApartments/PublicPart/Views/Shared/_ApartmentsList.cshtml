@model IList<RwaUtilities.Models.Apartment>

@if (Model.Count == 0)
{
    <div class="row justify-content-center mt-2">
        <div class="col-sm-8">
            <label><b>No apartments found :(</b></label>
        </div>
    </div>
}
@if (Model.Count != 0)
{
    foreach (var apartment in Model)
    {
        @Html.Partial("_ApartmentCard", apartment)
    }
}

